{
	"name": "adityadarma/laravel-jwt-sso",
	"description": "Generate JWT token for laravel",
	"homepage": "https://github.com/adityadarma/laravel-jwt-sso",
	"type": "library",
	"license": "MIT",
	"authors": [
		{
			"name": "Aditya Darma",
			"email": "adhit.boys1@gmail.com"
		}
	],
	"require": {
		"php": "^8.0",
		"ext-openssl": "*"
	},
	"autoload": {
		"psr-4": {
			"AdityaDarma\\LaravelJwtSso\\": "src/"
		}
	},
	"extra": {
		"laravel": {
			"providers": [
				"AdityaDarma\\LaravelJwtSso\\LaravelJwtSsoServiceProvider"
			],
			"aliases": {
				"Jwt": "Jwt"
			}
		}
	},
	"config": {
		"sort-packages": true,
		"preferred-install": "dist",
		"optimize-autoloader": true
	},
	"minimum-stability": "dev"
}